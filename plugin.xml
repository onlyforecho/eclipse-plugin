<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
	<extension point="org.eclipse.jdt.ui.javaCompletionProposalComputer"
	   id="textual_proposals"
	   name="Text Proposals">
